{
  "name": "@qpp/schema-inferrer",
  "version": "0.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean --external node-sql-parser",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "vitest run"
  },
  "peerDependencies": {
    "node-sql-parser": "^5.0.0"
  },
  "devDependencies": {
    "@qpp/eslint-config": "workspace:^",
    "eslint": "^9.39.2",
    "node-sql-parser": "^5.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
