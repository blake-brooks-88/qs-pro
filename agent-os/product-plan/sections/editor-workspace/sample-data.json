      "id": "q-102",
      "name": "Bounce Cleanup Script",
      "folderId": "f-6",
      "content": "SELECT \n  s.EmailAddress, \n  b.BounceCategory \nFROM [_Subscribers] s\nJOIN [_Bounce] b ON s.SubscriberKey = b.SubscriberKey\nWHERE b.BounceCategory = 'Hard Bounce'",
