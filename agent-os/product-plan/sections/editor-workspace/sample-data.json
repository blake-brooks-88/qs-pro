      "name": "Active Subscribers 30d",
      "folderId": "f-4",
      "content": "SELECT \n  SubscriberKey, \n  EmailAddress, \n  Status \nFROM [_Subscribers] \nWHERE Status = 'Active' \nAND DateJoined > DATEADD(day, -30, GETDATE())",
